<h1>All Bands</h1>
@if(@Model.Count == 0)
{
  <h3>You have not added any Bands</h3>
}
@foreach(var band in Model)
{
  <ul>
    <li><a href="/band/@band.GetId()">@band.GetName()</a> | <a href="/band/edit/@band.GetId()">Edit</a> | <a href="/band/delete/@band.GetId()">Delete</a></li>
  </ul>
}
<p><a href="/band/new">Add a new Band</a></p>
<p><a href="/">Return Home</a></p>
