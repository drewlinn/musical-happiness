<h1>All Venues</h1>
@if(@Model.Count == 0)
{
  <h3>You have not added any Venues</h3>
}
@foreach(var venue in Model)
{
  <ul>
    <li><a href="/venue/@venue.GetId()">@venue.GetName()</a> | <a href="/venue/edit/@venue.GetId()">Edit</a> | <a href="/venue/delete/@venue.GetId()">Delete</a></li>

  </ul>
}
<p><a href="/venue/new">Add a new Venue</a></p>
<p><a href="/">Return Home</a></p>
